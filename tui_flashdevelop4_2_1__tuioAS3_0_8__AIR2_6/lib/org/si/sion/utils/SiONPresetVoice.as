    // internal functions
    //--------------------------------------------------
        // create new voice
        private function _new(key:String, name:String, voice:SiONVoice) : void {
            voice.name = name;
            _categolyList.push(voice);
            this[key] = voice;
        }
        
        
        // create new OPN voice
        private function _OPN(key:String, name:String, ...args) : void {
            var voice:SiONVoice = new SiONVoice();
            voice.paramOPN = args;
            voice.name = name;
            _categolyList.push(voice);
            this[key] = voice;
        }
        
        
        // create new MA3 voice
        private function _MA3(key:String, name:String, ...args) : void {
            var voice:SiONVoice = new SiONVoice();
            voice.paramMA3 = args;
            voice.name = name;
            _categolyList.push(voice);
            this[key] = voice;
        }
        
        
        // register categoly
        private var _categolyList:Array;
        private function _categoly(key:String) : void {
            _categolyList = [];
            _categolyList["name"] = key;
            categolies.push(_categolyList);
            this[key] = _categolyList;
        }
    }
